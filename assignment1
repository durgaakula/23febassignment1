l=[]
n=int(input('enter n'))
for i in range(n):
    e=int(input())
    l.append(e)
max=l[0]
for i in range(1,n):
    if l[i]>max:
       max =l[i]
for i in range(n):
    e=int(input())
    l.append(e)
min=l[0]
for i in range(1,n):
    if l[i]<min:
       min=l[i]
print('max:',max)
print('min:',min)


